{
  "name": "scully-universal",
  "version": "0.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/samvloeberghs/scully-universal"
  },
  "scripts": {
    "ng": "ng",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "lint-staged": "lint-staged",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "scully": "scully --scanRoutes --configFile .scully/scully.scully.config.js",
    "scully:serve": "scully serve --configFile .scully/scully.scully.config.js",
    "serve:dist:scully:static": "npx ws --port 4203 -d dist/apps/scully-static",
    "serve:dist:universal:static": "npx ws --port 4200 -d dist/apps/universal/browser",
    "dev:ssr": "nx run universal:serve-ssr",
    "serve:ssr": "node dist/apps/universal/server/main.js",
    "build:ssr": "nx build universal --prod && ng run universal:server:production",
    "prerender:universal": "npm run list-routes:universal && ng run universal:prerender --routesFile apps/universal/routes.txt",
    "list-routes:universal": "ts-node -P ./tools/tsconfig.tools.json ./tools/universal/generate-routes.ts",
    "minify:universal": "ts-node -P ./tools/tsconfig.tools.json ./tools/universal/optimize-html-files.ts ./dist/apps/universal/browser"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint-staged",
      "pre-push": ""
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.0",
    "@angular/common": "~9.1.0",
    "@angular/compiler": "~9.1.0",
    "@angular/core": "~9.1.0",
    "@angular/forms": "~9.1.0",
    "@angular/localize": "^9.1.3",
    "@angular/platform-browser": "~9.1.0",
    "@angular/platform-browser-dynamic": "~9.1.0",
    "@angular/platform-server": "~9.1.0",
    "@angular/router": "~9.1.0",
    "@nguniversal/express-engine": "^9.1.0",
    "@nrwl/angular": "~9.3.0",
    "@scullyio/init": "0.0.28",
    "@scullyio/ng-lib": "latest",
    "@scullyio/scully": "latest",
    "axios": "^0.19.2",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.11.1",
    "core-js": "^2.5.4",
    "express": "^4.15.2",
    "html-minifier": "^4.0.0",
    "normalize.css": "^8.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.5.0",
    "scully-plugin-minify-html": "^2.5.0",
    "uuid": "^8.0.0",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular/cli": "~9.1.0",
    "@angular/compiler-cli": "~9.1.0",
    "@angular/language-service": "~9.1.0",
    "@babel/core": "7.8.3",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^8.3.4",
    "@nguniversal/builders": "^9.1.0",
    "@nrwl/cypress": "~9.3.0",
    "@nrwl/jest": "~9.3.0",
    "@nrwl/nest": "~9.3.0",
    "@nrwl/node": "~9.3.0",
    "@nrwl/workspace": "~9.3.0",
    "@types/express": "^4.17.0",
    "@types/jest": "^25.1.4",
    "@types/node": "~8.9.4",
    "babel-loader": "8.0.6",
    "codelyzer": "~5.0.1",
    "css-loader": "^3.5.3",
    "cypress": "^4.3.0",
    "dotenv": "^6.2.0",
    "eslint": "^6.8.0",
    "husky": "^4.2.3",
    "jest": "^25.2.4",
    "jest-preset-angular": "^8.2.0",
    "lint-staged": "^10.1.2",
    "local-web-server": "^4.0.0",
    "prettier": "^2.0.5",
    "scully-plugin-disable-angular": "^4.0.1",
    "ts-jest": "^25.2.1",
    "ts-node": "^7.0.0",
    "tslint": "^6.0.0",
    "typescript": "~3.8.3"
  }
}
